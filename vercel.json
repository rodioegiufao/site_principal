{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/analisador_carimbo",
      "destination": "https://tudosobreeletrica.blog/analisador_carimbo.html",
      "permanent": true
    },
    {
      "source": "/analisador_carimbo.html", 
      "destination": "https://tudosobreeletrica.blog/analisador_carimbo.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
